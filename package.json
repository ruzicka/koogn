{
  "name": "exvalidator",
  "version": "0.1.0",
  "description": "Validate JS objects by example (not schema)",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "prepublish": "npm test && npm run build",
    "lint": "eslint src/ test/",
    "test": "cross-env NODE_ENV=test mocha --recursive",
    "test:watch": "cross-env NODE_ENV=test mocha --recursive --watch",
    "test:coverage": "cross-env NODE_ENV=test istanbul cover _mocha -- --recursive",
    "test:coveralls": "cross-env NODE_ENV=test istanbul cover _mocha -- --recursive && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruzicka/xvalidator.git"
  },
  "author": "David Ruzicka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruzicka/xvalidator/issues"
  },
  "homepage": "https://github.com/ruzicka/xvalidator#readme",
  "keywords": [
    "validator",
    "validation",
    "validate",
    "schema",
    "json",
    "jsonschema",
    "example"
  ],
  "dependencies": {
    "jsonschema": "^1.2.2",
    "lodash.isplainobject": "^4.0.6",
    "to-json-schema": "^0.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "5.0.0"
  },
  "files": [
    "lib"
  ]
}
